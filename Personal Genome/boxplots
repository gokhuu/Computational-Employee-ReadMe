import numpy as np 
import pandas as pd 
import matplotlib.pyplot as plt 
import seaborn as sns 

df = pd.read_csv('Organoid Files/all_autism_fpkm_t_threshold.csv',header=0,index_col=0)

features = df.iloc[:,1:]

cols = list(features.columns)

for i in range(15):
	temp = features.get(cols[i])
	
	sns.boxplot(temp)

plt.show()